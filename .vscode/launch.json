    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug main.go",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "cmd/casc/main.go",
            "args": [
                "-dir", "F:/myGames/Diablo III"
            ]
        },
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}",
            "args": [
                "-dir", "F:/myGames/Diablo III"
            ]
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${fileDirname}",
            "args": [
                "-dir", "F:/myGames/Diablo III"
            ]
        }
    ]
}